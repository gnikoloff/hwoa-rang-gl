{
  "name": "hwoa-rang-gl",
  "version": "0.0.1",
  "description": "Small webgl libary for personal needs",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "lint": "eslint src",
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "serve:examples": "cd docs/examples && npm run serve",
    "watch:examples": "cd docs/examples && npm run watch",
    "build:examples": "cd docs/examples && npm run start",
    "start:examples:dev": "run-p watch watch:examples serve:examples",
    "start:examples": "run-s build build:examples serve:examples",
    "lint:examples": "eslint docs/examples",
    "build:docs": "typedoc --out docs/docs src/ --readme docs/README.md",
    "serve:docs": "http-serve docs/docs -p 8081",
    "start:docs": "run-p build:docs serve:docs"
  },
  "keywords": [
    "webgl",
    "glsl",
    "3d"
  ],
  "author": "Georgi Nikolov",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-tsdoc": "^0.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.28.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "tslib": "^2.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/webgl-ext": "0.0.32",
    "dat.gui": "^0.7.7",
    "gl-matrix": "^3.3.0",
    "http-serve": "^1.0.1",
    "lodash.throttle": "^4.1.1",
    "stats-js": "^1.0.1",
    "typedoc": "^0.20.28"
  }
}
